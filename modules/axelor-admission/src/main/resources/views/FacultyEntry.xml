<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
  <form name="faculty-entry-form" title="Faculty entry"
    model="com.axelor.admission.db.FacultyEntry">
    <panel title="Faculty Entry Detail">
      <field name="faculty" />
      <field name="seats" />
      <field name="college" hidden="true"/>
    </panel>
  </form>
  <grid name="faculty-entry-grid" title="Faculty entries"
    model="com.axelor.admission.db.FacultyEntry">
    <field name="faculty" />
    <field name="seats" />
  </grid>
</object-views>
